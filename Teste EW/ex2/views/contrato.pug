extends layout 
block content 
    .w3-card-4 
        header.w3-container.w3-teal 
            h1= contrato["_id"]
            h2= contrato["nAnuncio"]
        .w3-container 
            p Número de Anuncio: #{contrato["nAnuncio"]}
            p Tipo de Procedimento: #{contrato["tipoprocedimento"]}
            p Objeto Contrato: #{contrato["objectoContrato"]}
            p Data de Publicação: #{contrato["dataPublicacao"]}
            p Data de Contrato: #{contrato["dataCelebracaoContrato"]}
            p Preço Contratual: #{contrato["precoContratual"]}
            p Prazo de Execução: #{contrato["prazoExecucao"]}
            p NIPC da Entidade Comunicante: #{contrato["NIPC_entidade_comunicante"]}
            p Entidade Comunicante: #{contrato["entidade_comunicante"]}
            p Fundamentação: #{contrato["fundamentacao"]}
            button 
                a(href='/') Início             

        
        footer.w3-container.w3-teal 
            h5 Generated by Inês Castro::EngWeb2024 in #{d}
